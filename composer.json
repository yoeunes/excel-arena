{
    "name": "salmayno/excel-arena",
    "description": "An application that format an excel file to be used inside arena.",
    "keywords": ["arena", "excel"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name": "Salma Mourad",
            "email": "mourad.salma95@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "alt-three/bus": "^3.0",
        "laravel-zero/framework": "^3.9.2",
        "maatwebsite/excel": "^2.1",
        "nunomaduro/collision": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "symfony/var-dumper": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["excel-arena"]
}
